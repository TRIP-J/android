<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.ui.LoginActivity">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/logoImageView"
        android:layout_width="@dimen/dp_0"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_launcher_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="132dp"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginEnd="@dimen/dp_20"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/kakaoLoginImageView"
        android:layout_width="@dimen/dp_0"
        android:layout_height="@dimen/dp_56"
        app:layout_constraintStart_toStartOf="@id/logoImageView"
        app:layout_constraintEnd_toEndOf="@id/logoImageView"
        app:layout_constraintTop_toBottomOf="@id/logoImageView"
        android:layout_marginTop="208dp"
        android:src="@drawable/kakao_button"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/naverLoginImageView"
        android:layout_width="@dimen/dp_0"
        android:layout_height="@dimen/dp_56"
        app:layout_constraintTop_toBottomOf="@id/kakaoLoginImageView"
        app:layout_constraintStart_toStartOf="@id/logoImageView"
        app:layout_constraintEnd_toEndOf="@id/logoImageView"
        android:src="@drawable/naver_button"
        android:layout_marginTop="@dimen/dp_16"/>

</androidx.constraintlayout.widget.ConstraintLayout>